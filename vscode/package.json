{
    "name": "xteko-sync",
    "displayName": "xteko-sync",
    "description": "",
    "version": "1.1.2",
    "publisher": "cyan",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onDebug",
        "onLanguage:javascript"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "xteko-sync configuration",
            "properties": {
                "host": {
                    "type": "string",
                    "default": "",
                    "description": "Fill this with your connect host."
                }
            }
        },
        "commands": [
            {
                "command": "extension.setHost",
                "title": "Set Host"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "extension.setHost"
                }
            ]
        }
    }
}